	<parameter name="evaluation_id" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
	s.smft_subject_name AS subject_name,
	e.smft_subject_id AS subject_id,
	e.smft_evaluation_id AS evaluation_id,
    e.smft_evaluation_number AS evaluation_number,
	e.smft_description AS evaluation_description,
    q.smft_question_text AS question_text,
	q.smft_question_number AS question_number,
    a.smft_response_text AS response_text,
	a.smft_iscorrect AS iscorrect
FROM
    smft_evaluation e
INNER JOIN
	smft_subject s ON e.smft_subject_id = s.smft_subject_id
INNER JOIN
    smft_question q ON e.smft_evaluation_id = q.smft_evaluation_id
LEFT JOIN
    smft_answer a ON q.smft_question_id = a.smft_question_id
 WHERE
	e.smft_evaluation_id =  $P{evaluation_id}]]>
	</queryString>
